  <tr>
    <td><%=h "#{listitem.name}" %></td>
    <td><%=h "#{listitem.address}" %></td>
    <td><%=h "#{listitem.phone}" %></td>
    <td class="show"><%= link_to _('Show'), :action => 'show', :id => listitem %></td>
