<script src="http://maps.google.com/maps?file=api&v=2&key=<%= h GOOGLE_MAPS_API_KEY%>"
    type="text/javascript"></script>

<%= render :partial => 'flash' %>
<%= error_messages_for :item %>
<%= error_messages_for :picture %>

<div class="edititem">
<% form_tag 'create', :multipart => true do %>
  <%= hidden_field :item, :id %>
  <p><label for= "item_created_at"><%= h _('created_at') %>:</label>
	<%= datetime_select :item, :created_at, :use_month_numbers => true %>
  </p>
  <%= render :partial => 'form' %>
  <%= submit_tag _('Save changes') %>
<% end %>
 <!--   <%= select_datetime %> -->
</div>

